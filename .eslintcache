ilAndPassword(email, password);\n  };\n  //Google\n  const signInGoogle = async () => {\n    await auth.signInWithPopup(provider).catch((err) => alert(err.message));\n  };\n\n  return (\n    <Container component=\"main\" maxWidth=\"xs\">\n      <CssBaseline />\n      <div className={classes.paper}>\n        <Avatar className={classes.avatar}>\n          <LockOutlinedIcon />\n        </Avatar>\n        <Typography component=\"h1\" variant=\"h5\">\n        {isLogin ? \"ログイン\" : \"サインアップ\"}\n        </Typography>\n        <form className={classes.form} noValidate>\n          <TextField\n            variant=\"outlined\"\n            margin=\"normal\"\n            required\n            fullWidth\n            id=\"email\"\n            label=\"Email Address\"\n            name=\"email\"\n            autoComplete=\"email\"\n            autoFocus\n            value={email}\n            onChange={(e:React.ChangeEvent<HTMLTextAreaElement | HTMLInputElement>)=>setEmail(e.target.value)}\n          />\n          <TextField\n            variant=\"outlined\"\n            margin=\"normal\"\n            required\n            fullWidth\n            name=\"password\"\n            label=\"Password\"\n            type=\"password\"\n            id=\"password\"\n            autoComplete=\"current-password\"\n            value={password}\n            onChange={(e:React.ChangeEvent<HTMLTextAreaElement | HTMLInputElement>)=>setPassword(e.target.value)}\n          />\n\n          {/* --------ログイン-------- */}\n          <Button\n            fullWidth\n            variant=\"contained\"\n            color=\"primary\"\n            className={classes.submit}\n            startIcon={<EmailIcon />}\n            onClick={\n              isLogin\n                //ログインモードで入ってきた時\n                ? async () => {\n                    try {\n                      await signInEmail();\n                    } catch (err) {\n                      alert(err.message);\n                    }\n                }\n                //サインアップモードで入ってきた時\n                : async () => {\n                    try {\n                      await signUpEmail();\n                    } catch (err) {\n                      alert(err.message);\n                    }\n                  }\n            }\n          >\n            {isLogin? \"Login\":\"SignUp\"}\n          </Button>\n\n          {/* --------切り替えボタン-------- */}\n          <Grid container>\n              <Grid item xs>\n                <span\n                  className={styles.login_reset}\n                >\n                  パスワードお忘れの方?\n                </span>\n              </Grid>\n              <Grid item>\n                <span\n                  className={styles.login_toggleMode}\n                  onClick={() => setIsLogin(!isLogin)}\n                >\n                  {isLogin ? \"新規作成\" : \"ログイン\"}\n                </span>\n              </Grid>\n          </Grid>\n\n          {/* --------google--------*/}\n          <Button\n            fullWidth\n            variant=\"contained\"\n            color=\"primary\"\n            className={classes.submit}\n            onClick={signInGoogle}\n          >\n            Google\n          </Button>\n\n        </form>\n      </div>\n    </Container>\n  );\n}\n\nexport default Auth;","/Users/hongmuchan/Desktop/twitter-ts/src/components/Feed.tsx",[],{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","severity":1,"message":"52","line":2,"column":9,"nodeType":"53","messageId":"54","endLine":2,"endColumn":20},{"ruleId":"51","severity":1,"message":"55","line":4,"column":26,"nodeType":"53","messageId":"54","endLine":4,"endColumn":33},{"ruleId":"51","severity":1,"message":"56","line":11,"column":3,"nodeType":"53","messageId":"54","endLine":11,"endColumn":8},{"ruleId":"51","severity":1,"message":"57","line":15,"column":3,"nodeType":"53","messageId":"54","endLine":15,"endColumn":8},{"ruleId":"51","severity":1,"message":"58","line":16,"column":3,"nodeTyp